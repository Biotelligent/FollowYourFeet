<Alloy>
  <ItemTemplate class="feet-listitemtemplate" backgroundColor="Alloy.CFG.backgroundColorDark" name="menuItemTemplate" top="0" height="60" accessoryType="Ti.UI.LIST_ACCESSORY_TYPE_DISCLOSURE" >
    <View bindId="selectedview" id="selectedview" backgroundColor="white" top="0" left="0" width="4" height="60" layout="absolute" visible="false" zIndex="2" />
    <View class="feet-list-dark" bindId="subview" id="subview" top="4" left="10" right="10" height="59" layout="absolute" opacity="0.8" >
        <ImageView platform="ios" class="menuicon" bindId="icon" top="8" visible="false" />
        <ImageView platform="android" class="menuicon" bindId="icon" top="15" visible="false"/>
        <View bindId="textsubview" backgroundColor="transparent" top="15" left="10" right="10" height="59" layout="vertical">
          <Label class="feet-list-titlelabel" bindId="titleLabel" top="0" left="0" right="0" height="Ti.UI.SIZE"/>
          <Label class="feet-list-subtitlelabel" bindId="subtitleLabel" top="2" left="0" right="0" height="Ti.UI.SIZE" wordWrap="true"/>
        </View>
        <Label class="menucountlabel" bindId="count" visible="false" top="17" right="26"/>
      </View>
      <!--View bindId="separatorLine" left="0" right="0" top="59" height="1" backgroundColor="#444444"/-->
    </ItemTemplate>
</Alloy>